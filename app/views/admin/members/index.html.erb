<h2>Members</h2>
<div class="subnav">
  <div class="row">
    <div class="col-xs-6">
      <%= members_status_filter_button_group %>
    </div>
    <div class="col-xs-offset-4 col-xs-2">
      <div class="pull-right">
        <%= link_to new_admin_member_path, class: "btn btn-success" do %>
          <%= fa_icon "plus-circle" %>
          Add member
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class="table-list-header">
  <div class="row">
    <div class="col-xs-4">
      <h5>Name</h5>
    </div>
    <div class="col-xs-3">
      <h5>Membership ID</h5>
    </div>
    <div class="col-xs-2">
      <h5>Status</h5>
    </div>
    <div class="col-xs-3">
      <h5>&nbsp;</h5>
    </div>
  </div>
</div>
<ul class="members table-list">
  <%= render partial: "admin/members/member", collection: @members %>
</ul>
<div class="paginate-container">
  <%= paginate @members %>
  <div class="paginate-info">
    <%= page_entries_info @members %>
  </div>
</div>
